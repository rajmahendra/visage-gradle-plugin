buildscript {        repositories {  		mavenCentral();          mavenRepo url: "http://visage-lang.github.com/visage-gradle-plugin/repository/"        }          dependencies {   		   classpath 'org.visage-lang:visage-gradle-plugin:0.5'       } }  if (!project.plugins.findPlugin(org.visage.gradle.plugin.templates.VisageTemplatePlugin))  { project.apply(plugin: org.visage.gradle.plugin.templates.VisageTemplatePlugin) }